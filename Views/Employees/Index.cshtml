@model IEnumerable<UtvecklartestAgioMVC.Models.Employee>

@{
    ViewBag.Title = "Utvecklartest Per Nordenbrink 2022-06-07";
}

<h2>Anställda</h2>

<p>
    @Html.ActionLink("Lägg upp ny anställd", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Förnamn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Efternamn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Personnummer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Anställningsnummer)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Förnamn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Efternamn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Personnummer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Anställningsnummer)
        </td>
        <td>
            @Html.ActionLink("Redigera", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detaljer", "Details", new { id=item.Id }) |
            @Html.ActionLink("Ta bort", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
